<Application x:Class="Stive.Client.App"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Stive.Client"
             StartupUri="Login.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <FontFamily x:Key="FontAwesome">/Fonts/fontawesome-webfonts.otf#Font Awesome 5 Free Solid</FontFamily>
                </ResourceDictionary>                
                <hc:ThemeResources/>
                <hc:Theme/>
            </ResourceDictionary.MergedDictionaries>
           
        </ResourceDictionary>

    </Application.Resources>

</Application>