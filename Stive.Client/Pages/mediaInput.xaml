<Window x:Class="Stive.Client.Pages.mediaInput"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stive.Client.Pages"
        mc:Ignorable="d"
        Title="mediaInput" Height="115" Width="388">
    <Grid Margin="0,0,0,-23" >

        <Label Content="URL :" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="urlInput" HorizontalAlignment="Left" Margin="10,31,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="367" Grid.ColumnSpan="2" Height="21"/>
        <Button Content="Valider" HorizontalAlignment="Left" Height="19" Margin="337,57,0,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
