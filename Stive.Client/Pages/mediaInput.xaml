<Window x:Class="Stive.Client.Pages.mediaInput"        
        xmlns:hc="https://handyorg.github.io/handycontrol"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Stive.Client.Pages"
        mc:Ignorable="d"
        Title="mediaInput" Height="130" Width="420">
    <Grid Margin="0,0,0,-23" >

        <Label Content="URL :" HorizontalAlignment="Left" Margin="10,5,0,0" VerticalAlignment="Top" Height="29"/>
        <TextBox x:Name="urlInput" HorizontalAlignment="Center" Margin="0,34,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="400" Height="21"/>
        <Button Content="Valider" Style="{hc:StaticResource ButtonSuccess}" HorizontalAlignment="Left" Height="32" Margin="332,64,0,0" VerticalAlignment="Top" Click="Button_Click" Width="73"/>
        <Button Content="Annuer" Style="{hc:StaticResource ButtonDanger}" HorizontalAlignment="Left" Height="32" Margin="247,64,0,0" Click="Button_annuler_Click" Width="85" VerticalAlignment="Top"/>
    </Grid>
</Window>
